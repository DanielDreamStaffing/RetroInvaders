<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MainActivity"
    android:clickable="true"
    android:background="@drawable/fhs_hd"
    >

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">

        <FrameLayout
            android:layout_width="300dp"
            android:layout_height="230dp"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="90dp"
            android:layout_gravity="right|top"
            android:background="#000000">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="1° Test       55,000"
                android:textColor="#FFFFFF"
                android:textSize="15pt"
                android:id="@+id/textPrimero"
                android:layout_gravity="left|top"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="15dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="2° Test       45,000"
                android:textColor="#FFFFFF"
                android:textSize="15pt"
                android:id="@+id/textSegundo"
                android:layout_gravity="left|top"
                android:layout_marginTop="50dp"
                android:layout_marginLeft="15dp"/>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="3° Test       35,000"
                android:textColor="#FFFFFF"
                android:textSize="15pt"
                android:id="@+id/textTercero"
                android:layout_gravity="left|top"
                android:layout_marginTop="95dp"
                android:layout_marginLeft="15dp"/>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="4° Test       25,000"
                android:textColor="#FFFFFF"
                android:textSize="15pt"
                android:id="@+id/textCuarto"
                android:layout_gravity="left|top"
                android:layout_marginTop="140dp"
                android:layout_marginLeft="15dp"/>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="5° Test       15,000"
                android:textColor="#FFFFFF"
                android:textSize="15pt"
                android:id="@+id/textQuinto"
                android:layout_gravity="left|top"
                android:layout_marginTop="185dp"
                android:layout_marginLeft="14dp"/>

        </FrameLayout>

        <ImageButton
            android:layout_width="160dp"
            android:layout_height="155dp"
            android:id="@+id/btnReturn"
            android:layout_gravity="left|bottom"
            android:background="@drawable/return_hd"/>

    </FrameLayout>
</RelativeLayout>
